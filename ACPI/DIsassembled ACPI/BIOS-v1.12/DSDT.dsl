                        Store (Zero, VCQL)
                        Store (Zero, VTIO)
                        Store (Zero, VMYH)
                        Store (Zero, VSTP)
                        Store (Zero, VCQH)
                        Store (Zero, VDCC)
                        Store (Zero, VSFN)
                        Store (Zero, VDMC)
                        Store (Zero, VFHP)
                        Store (Zero, VIFC)
                        Store (Zero, VMMC)
                        Store (Zero, VMSC)
                        Store (Zero, VPSC)
                        Store (Zero, VCSC)
                        Store (Zero, SMYH)
                        Store (Zero, SMMC)
                        Store (Zero, SPSC)
                        Store (Zero, CICF)
                        If (LGreater (CLVL, One))
                        {
                            PL1S (0x0198)
                        }

                        If (LEqual (MPID, One))
                        {
